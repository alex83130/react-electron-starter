{
  "name": "adsn-congres",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-scripts": "3.0.1"
  },
  "homepage": "./",
  "main": "./electron-starter.js",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron": "electron .",
    "electron-prod": "npm run build && npm run electron",
    "electron-dev": "cross-env ELECTRON_START_URL=http://localhost:3000 electron .",
    "electron-exe" : "electron-packager ./ app-name --ignore=/node_modules --ignore=/src --platform=win32 --arch=x64 --overwrite"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^4.1.0",
    "cross-env": "^5.2.0",
    "electron": "^5.0.1",
    "electron-packager": "^13.1.1",
    "wait-on": "^3.2.0"
  }
}
